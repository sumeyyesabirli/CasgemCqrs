@model CasgemCqrs.Cqrs.Results.GetProductUpdateByIdQueryResult
@{
    Layout = null;
}

<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

<div class="container">
    <form method="post">
        <br /><br />
        <label>Ürün Id</label>
        <input type="text" class="form-control" asp-for="ProductId" />
        <br />
        <label>Ürün Adı</label>
        <input type="text" class="form-control" asp-for="Name" />
        <br />
        <label>Ürün Markası</label>
        <input type="text" class="form-control" asp-for="Brand" />
        <br /> 
        <label>Ürün Stok</label>
        <input type="text" class="form-control" asp-for="Stock" />
        <br /> 
        <label>Ürün Fiyat</label>
        <input type="text" class="form-control" asp-for="Price" />
        <br />
        <label>Ürün Karegori</label>
        <input type="text" class="form-control" asp-for="Category" />
        <br />
        <button class="btn btn-success">Güncelle</button>
    </form>
</div>